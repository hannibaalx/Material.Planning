{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "ClientApp/src/assets/css/",
      "destination": "wwwroot/lib/css/",
      "files": [
        "bootstrap.min.css",
        "font-awesome.css",
        "font-awesome.min.css"
      ]
    },
    {
      "library": "ClientApp/src/assets/fonts/",
      "destination": "wwwroot/lib/fonts/",
      "files": [
        "fontawesome-webfont.eot",
        "fontawesome-webfont.svg",
        "fontawesome-webfont.ttf",
        "fontawesome-webfont.woff",
        "fontawesome-webfont.woff2",
        "FontAwesome.otf"
      ]
    }
  ]
}